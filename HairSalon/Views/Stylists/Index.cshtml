<h1>Hair Salon</h1>
<hr />

<h2>Stylists:</h2>

@if (Model.Count != 0)
{
  <ul>
    @foreach (var stylist in Model)
    {
      <li><a href="/stylists/@stylist.GetId()">@stylist.GetName()</a></li>
    }
  </ul>
}

<h4><a href="/stylists/new">Add a New Stylist</a></h4>
<h4><a href="/stylists/deleteall">Delete All Stylists</a></h4>
<h4><a href="/">Home</a></h4>