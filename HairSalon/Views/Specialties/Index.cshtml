<h1>Hair Salon</h1>
<hr />

<h2>Specialties:</h2>

@if (Model.Count != 0)
{
  <ul>
    @foreach (var specialty in Model)
    {
      <li><a href='/specialties/@specialty.GetId()'>@specialty.GetStyle()</a></li>
    }
  </ul>
}


